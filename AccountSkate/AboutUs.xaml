    </Grid>
    <UserControl.Background>
        <RadialGradientBrush>
            <GradientStop Color="#FF505050" Offset="0" />
            <GradientStop Color="#FF202020" Offset="1" />
        </RadialGradientBrush>
    </UserControl.Background>
</UserControl>
