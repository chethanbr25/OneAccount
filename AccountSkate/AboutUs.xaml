            <TextBlock Foreground="#FFD4D4D4" Height="20" HorizontalAlignment="Left" Margin="656,410,0,0" Name="textBlock12" Text="Kakkancherry, Calicut University P.O., " TextWrapping="Wrap" VerticalAlignment="Top" Width="282" />
            <TextBlock Foreground="#FFD4D4D4" Height="20" HorizontalAlignment="Left" Margin="656,428,0,0" Name="textBlock13" Text="Kerala, India - 673635" TextWrapping="Wrap" VerticalAlignment="Top" Width="282" />
            <TextBlock Foreground="#FFD4D4D4" Height="20" HorizontalAlignment="Left" Margin="656,454,0,0" Name="textBlock14" Text="Phone: +91 (0) 4943015006" TextWrapping="Wrap" VerticalAlignment="Top" Width="282" />
