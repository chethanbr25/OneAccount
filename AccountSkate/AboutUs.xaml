             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="486" d:DesignWidth="944">
    <Grid>
        <Grid Margin="0,2,0,0">
            <TextBlock Height="23" HorizontalAlignment="Left" Margin="359,163,0,0" Name="textBlock3" Text="Account Skate" VerticalAlignment="Top" Width="143" FontSize="18" FontFamily="Or" Foreground="White" />
